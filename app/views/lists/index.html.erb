<h1>All lists</h1>
<ul>
  <% @lists.each do |list| %>
    <%= link_to list_path(list) do %>
      <li>
        <%= list.name %>
        <% if list.photo.attached? %>
          <%= cl_image_tag list.photo.key, height: 300, width: 400, crop: :fill %>
        <% else %>
          <%= cl_image_tag("cld-sample-3.jpeg", width: 400, height: 300, crop: :fill) %>
        <% end %>
      </li>
    <% end %>
  <% end %>
</ul>
<%= link_to "Create new list", new_list_path %>
